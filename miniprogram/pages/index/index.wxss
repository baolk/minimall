.swiper_index swiper {
  width: 750rpx;
  height: 340rpx;
}
.swiper_index swiper image {
  width: 100%;
}
.cate_input {
  display: flex;
}
.cate_input navigator {
  flex: 1;
  padding: 30rpx;
}
.cate_input navigator image {
  width: 100%;
}
.floor_index .floor_group .floor_title {
  padding: 10rpx 0;
}
.floor_index .floor_group .floor_title image {
  width: 100%;
}
.floor_index .floor_group .floor_content navigator {
  float: left;
  width: 32.33%;
}
.floor_index .floor_group .floor_content navigator:nth-last-child(-n+3) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor_index .floor_group .floor_content navigator:nth-child(1),
.floor_index .floor_group .floor_content navigator:nth-child(2) {
  border-bottom: 10rpx solid #fff;
}
.floor_index .floor_group .floor_content navigator image {
  width: 100%;
  height: 100%;
}
